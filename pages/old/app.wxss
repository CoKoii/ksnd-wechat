/**app.wxss**/
@import '/style/weui.wxss';
.page{
  font-size: 14px;
  font-family: -apple-system-font,Helvetica Neue,Helvetica,sans-serif;
}
.page__bd {
  padding-top: 0.1px;
  min-height: 100vh;
  /* background-color: #EDEDED; */
  background-color: #555;
}

image{max-width:100%;max-height:100%}
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}
mp-cell {
  font-size: 14px;
}

/* 物料详情，车辆详情 */
.jk-detail {
  display: flex;
  flex-direction: column;
  background: #ffffff;
  font-size: 10pt;
  width: calc(100%-32px);
  padding: 9px 16px;
}

.jk-detail:not(:first-child) {
  margin-top: 16px;
}
.jk-detail .jk-row {
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: flex-start;
  padding: 3px 0px;
}
.jk-detail .jk-label {width: 70px;}
.jk-detail .jk-label:not(:first-child) {margin-left: 3px;}
.jk-detail .jk-text {flex: 1;color: rgba(0, 0, 0, 0.5);}
/* 水平分布 左对齐 上下居中 */
.jk-row {display: flex;flex-direction: row;justify-content: flex-start;align-items: center;width: 100%;}
/* 水平分布 两端对齐 */
.jk-row-between {display: flex;flex-direction: row;justify-content: space-between;align-items: center;width: 100%;}
/* 水平分布，自动换行 */
.jk-row-wrap {display: flex;flex-direction: row;justify-content: flex-start;align-items: center;flex-wrap: wrap;}
/* 水平分布，右对齐 上下居中 */
.jk-row-right {display: flex;flex-direction: row;justify-content: flex-end;align-items: center;}
/* 垂直分布 默认靠左 */
.jk-column {display: flex;flex-direction: column;justify-content: flex-start;}
/* 垂直分布 左右居中 */
.jk-column-center {display: flex;flex-direction: column;justify-content: flex-start;align-items: center;width: 100%;}
.jk-lab {width: 80px;}
.jk-text {flex: 1;color: rgba(0, 0, 0, 0.5);}
.jk-text-black {color: rgba(0, 0, 0, 0.5);}
.jk-text-desc {color: var(--weui-FG-1);}
/* 自定义面板样式 */
.jk-panel-bg {background: #f8f8f8;}
.jk-panel {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  background: #ffffff;
  font-size: 10pt;
  width: calc(100%-32px);
  padding: 16px;
}
.jk-panel-column {
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  background: #ffffff;
  font-size: 10pt;
  width: calc(100%-32px);
  padding: 16px;
}
.jk-panel-column:not(:first-child) {
  margin-top: 9px;
}
.jk-panel-1 {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  background: #ffffff;
  font-size: 10pt;
  width: calc(100%-32px);
  padding:0 10px;
}
.jk-panel-2{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  background: #ffffff;
  font-size: 10pt;
  width: calc(100%-32px);
  padding:5px 10px;
  border-bottom: 1px solid #E5E5E5;
}
.jk-panel:not(:first-child) {
  margin-top: 16px;
}
.jk-panel-content {
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  align-items: flex-start;
  flex: 1;
}
.jk-panel-content-row {
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: flex-start;
  flex-wrap: wrap;
  flex: 1;
}
.jk-panel-content:not(:first-child), .jk-panel .jk-panel-arrow {
  margin-left: 16px;
}

.jk-panel-content .jk-row {
  line-height: 46rpx;
}
/* 面板右部箭头 */
.jk-panel-arrow {
  width: 64px;
  height: 64px;
  align-items: center;
  justify-content: center;
  align-self: center;
}
.submit-btn{
  color:#fff!important;
  background-color: #FF560F!important;
}

.jk-panel-qr{
  width: 20px;
  height: 20px;
  align-items: center;
  justify-content: center;
  align-self: center;
  margin-left:30px;
}

.jk-title {
  flex: 1;
  font-size:12pt;
  color:#3F539E;
  padding:5px 0;
}

.jk-type {
  flex: 1;
  color: rgba(0, 0, 0, 0.5);
  right:0;
}

.jk-text-black {
  /* flex: 1; */
  color: #333;
}

.jk-row button.jk-button-bottom {
  border-radius: 10rpx;
  font-size: 14px;
  margin: 16px;
  flex: 1;
}

